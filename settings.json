{
/************* vim配置 *************/
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.surround": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,

  //插入模式键位绑定
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  //normal模式键位绑定
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "s"],
      "commands": [":w!"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["<leader>", "sq"],
      "commands": [":wq!"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    }
  ],

  //命令模式键位绑定
  "vim.commandLineModeKeyBindingsNonRecursive": [],

  //可视模式键位绑定
  "vim.operatorPendingModeKeyBindings": [],

  "vim.handleKeys": {
  }
}